    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.4.5",
    "power-assert": "^1.3.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
